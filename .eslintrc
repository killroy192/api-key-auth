{
  "env": {
    "node": true
  },
  "plugins": [
    "security",
    "import"
  ],
  "extends": [
    "airbnb-base",
    "plugin:security/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "import/no-unresolved": [2, {"esm": true}],
    "import/named": 2,
    "import/namespace": 2,
    "import/default": 2
  }
}